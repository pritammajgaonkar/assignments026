'''
#8.
def unique(a):
    return list(set(a))

a=[1,2,2,3,3,3,4,4,5,5,5]
b=unique(a)
print(b)

'''
'''
#9.

def prime(no):
    flag=True
    for i in range (2,no):
        if no%i==0:
            flag=False
            break
    if(flag==True):
        print("prime")
    else:
        print("not prime")


no=int(input("Enter a no:"))
prime(no)
'''
'''
#10.

def even(a):
    for i in a:
        if i%2==0:
            print(i)

a=[1,2,3,4,5,6,7,8,9]
even(a)
'''
