#11.

no=int(input("enter the number:"))
sum=0
for i in range(1,no+1,1):
    if no%i==0:
        sum=sum+i
        sum1=sum/2
if sum1==no:
    print("perfect")
else:
    print("not perfect")
print(sum1)

